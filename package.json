{
	"name": "@iconify/json",
	"description": "Iconify icons collection in JSON format",
	"license": "MIT",
	"version": "1.1.401",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/iconify/collections-json.git"
	},
	"files": [
		"dist",
		"json",
		"lib",
		"collections.json",
		"collections.md",
		"composer.md",
		"readme.md"
	],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"homepage": "https://iconify.design/icon-sets/",
	"bugs": "https://github.com/iconify/collections-json/issues",
	"scripts": {
		"build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts",
		"test-esm": "yarn build && jest --clearCache && cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --config=jest.config.esm.ts",
		"test-cjs": "yarn build && jest --clearCache && jest --config=jest.config.cjs.ts"
	},
	"dependencies": {
		"upath": "^2.0.1"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^0.9.0",
		"@types/jest": "^27.0.1",
		"@types/node": "^16.9.1",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"esno": "^0.9.1",
		"jest": "^27.2.0",
		"jest-each": "^27.2.0",
		"ts-jest": "^27.0.5",
		"ts-node": "^10.2.1",
		"tsup": "^4.14.0",
		"typescript": "^4.4.3"
	}
}
